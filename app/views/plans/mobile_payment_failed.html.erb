  <html lang=en>
  <head>
  <meta charset="UTF-8" />
  <title>Shemaroo</title>
  <meta name="ctest" content="ctest" /
  ><link href="/assets/images/favic.ico" rel="shortcut icon" />    
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0"  />

   <!-- Latest compiled and minified CSS -->
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css">
  <link href='http://fonts.googleapis.com/css?family=Roboto' rel='stylesheet' type='text/css'>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
  <script type="text/javascript" src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js"></script>
  <script type="text/javascript" src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script>
  <style type="text/css">
    /* Font integration */
    
    @font-face {    
      font-family: 'condensed-extra-bold';
      src: url('/fonts/EncodeSansCondensed-ExtraBold.ttf');
    }
    @font-face {    
      font-family: 'condensed-light';
      src: url('/fonts/EncodeSansCondensed-Light.ttf');
    }
    @font-face {
      font-family: 'condensed-medium';
      src: url('/fonts/EncodeSansCondensed-Medium.ttf');
        
    }
    @font-face {
      font-family: 'condensed-semibold';
      src: url('/fonts/EncodeSansCondensed-SemiBold.ttf');
    }
    .font-light {
      font-family: 'condensed-light';
    }
    .font-medium {
      font-family: 'condensed-medium';
    }
    .font-semibold {
      font-family: 'condensed-semibold';
    }
    .font-extrabold {
      font-family: 'condensed-extra-bold';
    }
    body {
      background-color: #161616;
      float: left;
      font-family: 'condensed-semibold';
      font-size: 16px;
      margin: 0;
      padding: 0;
      height: 100%;
      width: 100%;
    }
    .container {
      max-width: 1366px;
      margin: 0 auto;
      padding: 0;
    }
    .wrapper {
      background-color: #161616;
      float: left;
      width: 100%;
    }
    .inside-wrapper {
      background-color: #fff;
      float: left;
      min-height: 100%;
      padding-top: 25px;
      width: 100%;
    }
    section {
      float: left;
      width: 100%;
    } 
		.no_content_plans img.status_image {
		  width: 166px;
		  margin-top: 20px;
		  margin-bottom: 30px;
		}
		.transation_status_page {
		  align-items: center;
		  display: flex;
		  justify-content: center;
		}
		.transation_status_page h2 {
	    color: #000000;
	    font-size: 1.5em;
	    line-height: 1.25em;
	    margin-bottom: 13px !important;
		}
		.transation_status_page .font-medium {
		  color: #666;
		  margin-bottom: 0px;
		  margin-top: 0px;
		}
		.transation_status_page .font-light {
		  color: #777;
		  margin-bottom: 20px;
		  margin-top: 3px;
		}
		.transation_status_page .radius-button {
		  border: 1px solid #D51E54;
		  border-radius: 20px;
		  color: #D41F54;
		  display: inline-block;
		  font-family: 'condensed-semibold';
		  font-size: 0.875em;
		  margin-top: 10px;
		  padding: 13px 18px;
		  text-decoration: none;
		  text-transform: uppercase;
		}
		.transation_status_page .radius-button:hover {
		  text-decoration: none;
		}
		.no-items-found {
		  align-items: center;
		  display: flex;
		  flex-direction: column;
		  flex-wrap: wrap;
		  height: 100%;
		  justify-content: center;
		}
		.no-content-wrap {
		  margin: 0 auto;
		  max-width: 300px;
		  padding-bottom: 114px;
		}
		.no_content_plans {
		  max-width: 340px;
		}
		.no-content-wrap img.search-no-results-image {
		  width: 166px;
		  margin-top: 114px;
		  margin-bottom: 30px;
		}
		.no-content-wrap p {
		  font-size: 0.875em;
		}
    .margin-bottom-0 {
      margin-bottom: 0px !important
    }
    .margin-bottom-15 {
      margin-bottom: 15px !important
    }
  </style>
</head>
<body>
	<div class="wrapper">
	  <div class="container">
	    <div class="inside-wrapper">
        <section class="transation_status_page">
          <div class="row">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col- no-content-wrap no_content_plans text-center">
              <h2 class="block margin-bottom-10">Transaction Failed</h2>
              <img src="/assets/transaction_failed.svg" class="status_image" alt="" title="">
              <p class="font-light margin-bottom-15">Your payment of Rs. 45 could not be processed</p> 
              <!-- <p class="font-light margin-bottom-15">Reason: <%#=@resp["error"]["message"]["message"]%>&nbsp;&nbsp;(Error: <%#=@resp["error"]["code"]%>)</p>  -->
              <p class="font-light margin-bottom-0">Transation ID</p> 
              <p class="margin-bottom-0 font-medium margin-bottom-15"><strong>999999</strong></p>
              <a href="/plans" class="radius-button margin-bottom-25" id="">Try again</a>
            </div>
          </div>
        </section>
      </div>
    </div>
  </div>
  <script>
  var window_height = window.innerHeight;
  $('.inside-wrapper').css('min-height', window_height);
</script>
</body>
</html>