<section class="onboarding-bg min-height-div padding-top-85 padding-top-145">
  <div class="onboarding-payment-contianer margin-bottom-30">
    <div class="row">
      <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col- onboarding-box-wrap margin-bottom-20 margin-top-25">
        <h2 class="block margin-bottom-10">Registered Devices</h2>
        <div class="w-100 pull-left tile-box-shadow padding-left-30 settings_page padding-right-30 padding-bottom-15 padding-top-25 registered_devices_page rounded">
          <ul class="list-none padding-left-0 pull-left w-100 font-light">
            <li class="pull-left w-100 border-bottom iphone_device">
              <img src="/assets/iphone_icon.svg" class="pull-left" alt="" title=""><span class="pull-left normal_txt">iPhone</span>
              <span class="pull-right normal_txt"><a href="javascript:;" class="text-uppercase font-medium">remove</a></span>
            </li>
            <li class="pull-left w-100 border-bottom tv_device">
              <img src="/assets/tv_icon.svg" class="pull-left" alt="" title=""><span class="pull-left normal_txt">Mi TV</span>
              <span class="pull-right normal_txt"><a href="javascript:;" class="text-uppercase font-medium">remove</a></span>
            </li>
            <li class="pull-left w-100 border-bottom android_device">
              <img src="/assets/android_phone_icon.svg" class="pull-left" alt="" title=""><span class="pull-left normal_txt">Preet's Android Phone</span>
              <span class="pull-right normal_txt"><a href="javascript:;" class="text-uppercase font-medium">remove</a></span>
            </li>
          </ul>          
        </div>
      </div>
    </div>
  </div>
  <%=render "layouts/page_height_setting"%>
